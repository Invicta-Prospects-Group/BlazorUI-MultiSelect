@page "/"

@using StateManagementInterface
@using StateManagement.Fields.Actions
@using StateManagement.Icons.Actions

@inject IDispatcher Dispatcher

@inject IState<IFieldState> _fieldState
@inject IState<IIconState> _iconState

@foreach (var field in _fieldState.Value.Fields)
{
    <DynamicComponent Type="typeof(DynamicPaxSelectorMulti)" Parameters="@(new Dictionary<string, object>(){{"FieldId", field.Id}})"></DynamicComponent>
}